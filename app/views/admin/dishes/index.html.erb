
<h2><%= @category.name %></h2>

<div class="table-responsive">
<table class="table table-bordered">
	<thead>
		<tr>
			<th>Name</th>
			<th>Money($)</th>
		</tr>
	</thead>
    <tbody>
    	<% @category.dishes.each do |dish| %>
    	<tr>
    		<td> <%= link_to dish.name, admin_category_path(dish) %> </td>
    		<td> <%= dish.money %> </td>
    	</tr> 	
    	<%end%>
    </tbody>
</table>
</div>
<%= link_to 'Create a new Dish', admin_category_dishes_path(@category) %> | 
<%= link_to 'Back', admin_categories_path %>