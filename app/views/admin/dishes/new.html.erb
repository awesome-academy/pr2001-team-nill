<% provide(:title, 'New Dish') %>
<h1>Create dish</h1>
<div class="row">
<div class="col-md-6 col-md-offset-3">
<%= form_for [:admin, @category, @dish] do |f| %>
<%= f.label :name %>
<%= f.text_field :name,class: 'form-control' %>
<%= f.label :money %>
<%= f.text_field :money,class: 'form-control' %>
<%= f.submit "Create category", class: "btn btn-primary" %>
<% end %>
</div>
</div>

