<% provide(:title, "Edit user") %>
<h1>Update user</h1>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for([:admin, @user ]) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :email %>
      <%= f.text_field :email %>
      <%= f.submit "Save changes", class: "btn btn-primary" %>
		<% end %>
	</div>
</div>