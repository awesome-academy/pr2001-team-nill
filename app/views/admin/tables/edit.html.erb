<% provide(:title, "Edit table") %>
<h1>Update table</h1>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for([:admin, @table ]) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :status %>
      <%= f.text_field :status %>
      <%= f.submit "Save changes", class: "btn btn-primary" %>
		<% end %>
	</div>
</div>