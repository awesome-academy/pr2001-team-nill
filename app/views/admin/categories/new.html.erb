<% provide(:title, 'New Category') %>
<h1>Create category</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @category] do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.submit "Create category", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
