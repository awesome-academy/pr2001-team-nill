<% provide(:title, "Edit dish") %>
<h1>Update dish</h1>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for([:admin, @category ]) do |f| %>
			<%= f.label :name %>
			<%= f.text_field :name, class: 'form-control' %>
			<%= f.submit "Save changes", class: "btn btn-primary" %>
		<% end %>
	</div>
</div>